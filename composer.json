{
    "name": "hyperf/amqp",
    "description": "A amqplib for hyperf.",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "hyperf",
        "amqp"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "hyperf/framework": "dev-master",
        "hyperf/pool": "dev-master",
        "php-amqplib/php-amqplib": "^2.7"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\Amqp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\Amqp\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
